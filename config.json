{
  "log": {
    "loglevel": "none"
  },
  "dns": {
    "servers": [
      "https+local://8.8.8.8/dns-query",
      {
        "address": "https://1.1.1.1/dns-query",
        "domains": [
          "geosite:ru-blocked"
        ]
      }
    ],
    "queryStrategy": "UseIPv4",
    "disableFallbackIfMatch": true
  },
  "inbounds": [
    {
      "tag": "tproxy",
      "port": 12345,
      "protocol": "dokodemo-door",
      "settings": {
        "network": "tcp,udp",
        "followRedirect": true
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      },
      "streamSettings": {
        "sockopt": {
          "tproxy": "tproxy"
        }
      }
    }
  ],
  "outbounds": [
    {
      "tag": "direct",
      "protocol": "direct",
      "settings": {
        "domainStrategy": "UseIPv4"
      }
    },
    {
      "tag": "proxy",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "your.server.ipv4.address",
            "port": 443,
            "users": [
              {
                "id": "your-user-uuid-on-server",
                "encryption": "none",
                "flow": "xtls-rprx-vision"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "raw",
        "security": "reality",
        "realitySettings": {
          "fingerprint": "chrome",
          "serverName": "your.domain.com",
          "publicKey": "your_public_key",
          "shortId": "",
          "spiderX": ""
        }
      }
    },
    {
      "tag": "dns",
      "protocol": "dns"
    }
  ],
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "rules": [
      {
        "inboundTag": [
          "tproxy"
        ],
        "port": 53,
        "outboundTag": "dns"
      },
      {
        "ip": [
          "1.1.1.1"
        ],
        "port": 443,
        "outboundTag": "proxy"
      },
      {
        "ip": [
          "geoip:ru-blocked-community",
          "geoip:re-filter"
        ],
        "outboundTag": "proxy"
      },
      {
        "domain": [
          "geosite:ru-blocked"
        ],
        "outboundTag": "proxy"
      }
    ]
  }
}
